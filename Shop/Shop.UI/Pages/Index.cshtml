                        <p class="title-desc">Преобретайте новинки до доступной цене</p><!-- End .title-desc -->
                    </div><!-- End .heading -->

                    <div class="owl-carousel owl-Products owl-simple">
 
                            <div class="product item"  v-for="(product, index) in products" @@mouseenter="selectProduct(product, index)" name="product"
                             v-bind:hidden="!product.isActive" style="overflow: unset">
                                <div class="row justify-content-start">

                                    <figure class="product-media">
                                        <span class='product-label label-top' v-if="product.isTrending">Хит</span>
                                        <span class='product-label label-new' v-if="product.isNew">New</span>
                                        <span class='product-label label-sale' v-if="saleLabel(product, index)[0]">{{saleLabel(product, index)[1]}}%</span>
                                        <a :href="returnLink(product.name)">
                                            <img class="product-image shop" id="mainImage" :src="'/images/productsfolder/' + stocks[index].propImage"  style="height: 350px; width: 300px; object-fit: fill;"/>
                                        </a>

                                        <div class="product-action action-icon-top">
                                            <a class="btn-product btn-cart" @@click="selectProduct(product, index); addOneToCart()" title="В корзину"><span>В корзину</span></a>
                                            <a class="btn-product btn-wishlist" title="Wish" @@click="selectProduct(product, index); addOneToWish() " id="WishAdd"><span>В избранное</span></a>
                                        </div><!-- End .product-action -->

                                    </figure><!-- End .product-media -->
                                    <div class="sortingRating" style="display:none">{{product.avRating}}</div>
                                    <div class="sortingValue" style="display:none">{{stocks[index].value}}</div>

                                    <div class="product-body">
                                        <div class="product-cat">
                                            <a :href="returnLink(product.name)">{{product.category}}</a>
                                        </div>

                                        <h3 class="product-title"><a :href="returnLink(product.name)">{{product.description}} {{product.name}}</a></h3>

                                        <div class="product-price">
                                            <span class="new-price" v-bind:hidden="stocks[index].isOnSale <= 0">{{newValue(index)}} р.</span>
                                            <span class="old-price" v-bind:hidden="stocks[index].isOnSale <= 0"><s>{{stocks[index].value}} р.</s></span>
                                            <span name="mainValue" v-bind:hidden="stocks[index].isOnSale > 0">{{stocks[index].value}} р.</span>
                                        </div>

                                        <div class="ratings-container">
                                            <div class="ratings">
                                                <div class="ratings-val" id="starRat" v-bind:style="{width: (product.avRating*20)+'%'}" style="width: 0%"></div>
                                            </div>
                                            <span class="ratings-text" style="position:absolute; right: 16px;">{{product.review.length}} <i class="fa fa-comments"></i></span>
                                        </div>

                                        <div class="details-filter-row details-row-size" name="haveColors" style="justify-content: center; margin:auto" v-if="activeIndicate(product)">

                                            <div class="product-nav product-nav-dots" v-for="(stock, indexStock) in products[index].stock" v-if="stock.isActive===true">
                                                <a class="" name="colors" @@click="changeOptions(indexStock, index), selectStock(stock, indexStock)" v-bind:style="{background: stock.color}" style="background: #000000; margin: 3px; border: .01em solid rgb(0,0,0,0.05); outline: .2rem solid #fff;" title=""></a>

                                            </div>
                                        </div>

                                        <div v-else class="product-nav product-nav-dots" name="notColors" style="margin-bottom: 38px;"></div>
                                    </div><!-- End .product-body -->
                                </div>
